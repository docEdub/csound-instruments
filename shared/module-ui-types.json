{
    "uiTypes": [{
        "type": "module",
        "comment": "eval '(json['name'])'",
        "extends": "styled-group",
        "width": "eval '(Constants.Module.Width)'",
        "border": 1,
        "channel-prefix": "eval '(json['name'])'",
        "layout": "TopToBottom",
        "row-height": "eval '(tryInherit() ?? 27)'",
        "row-label-width": "eval '(tryInherit() ?? '40%')'",
        "row-value-width": "eval '(tryInherit() ?? '60%')'"
    }, {
        "type": "module-checkbox",
        "extends": "styled-group",
        "width": "100%",
        "height": "eval '(inherit('row-height'))'",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "80%",
            "height": "100%",
            "text": "eval '(tryInherit('label') ?? '')'",
            "align": "left"
        }, {
            "type": "styled-checkbox",
            "width": "20%",
            "height": "100%",
            "channel": "eval '(inherit('channel-prefix') + '::' + inherit('value-channel'))'",
            "checked": "eval '(inherit('value-default'))'"
        }]
    }, {
        "type": "module-combobox",
        "extends": "styled-group",
        "width": "100%",
        "height": "eval '(inherit('row-height'))'",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "eval '(inherit('row-label-width'))'",
            "height": "100%",
            "text": "eval '(tryInherit('label') ?? '')'",
            "align": "left"
        }, {
            "type": "styled-combobox",
            "width": "eval '(inherit('row-value-width'))'",
            "height": "100%",
            "items": "eval '(inherit('value-items'))'",
            "channel": "eval '(inherit('channel-prefix') + '::' + inherit('value-channel'))'"
        }]
    }, {
        "type": "module-group",
        "extends": "styled-group",
        "width": "eval '(Constants.Module.Width)'",
        "height": "100%",
        "border": 0,
        "layout": "TopToBottom"
    }, {
        "type": "module-label",
        "extends": "styled-label",
        "width": "100%",
        "height":"eval '(inherit('row-height'))'",
        "text": "eval '(json['label'])'",
        "align": "left"
    }, {
        "type": "module-main-label-and-enabled-checkbox",
        "extends": "styled-group",
        "comment": "Main Label",
        "width": "100%",
        "height": "eval '(1.333 * inherit('row-height'))'",
        "border": 0,
        "padding-top": "eval '(inherit('row-height') * 0.167)'",
        "padding-left": 8,
        "padding-right": 5,
        "layout": "TopToBottom",
        "background":"eval '(Constants.Module.Color.MainLabel)'",
        "children": [{
            "type": "module-checkbox",
            "label": "eval '(inherit('name'))'",
            "value-channel": "eval '(inherit('value-channel'))'",
            "value-default": "eval '(inherit('value-default'))'"
        }]
    }, {
        "type": "module-number-box-slider",
        "extends": "styled-group",
        "width": "100%",
        "height": "eval '(inherit('row-height'))'",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "eval '(inherit('row-label-width'))'",
            "height": "100%",
            "text": "eval '(tryInherit('label') ?? '')'",
            "align": "left"
        }, {
            "type": "styled-number-box-slider",
            "width": "eval '(inherit('row-value-width'))'",
            "height": "100%",
            "value-range": "eval '(inherit('value-range'))'",
            "value-channel": "eval '(inherit('channel-prefix') + '::' + inherit('value-channel'))'"
        }]
    }, {
        "type": "module-row-group",
        "extends": "styled-group",
        "width": "100%",
        "height": "eval '((json['row-count'] + 0.5) * inherit('row-height'))'",
        "border": 0,
        "padding-top": "eval '(inherit('row-height') * 0.25)'",
        "padding-left": 8,
        "padding-right": 5,
        "layout": "TopToBottom"
    }, {
        "type": "module-row-spacer",
        "extends": "styled-spacer",
        "width": "100%",
        "height": "eval '((tryInherit('row-height') ?? 27) / 2)'"
    }]
}
