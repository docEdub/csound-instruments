{
    "uiTypes": [{
        "type": "module",
        "comment": "(json['name'])",
        "extends": "styled-group",
        "width": "(constants.module.Width)",
        "border": 1,
        "channel-prefix": "(inherit('channel-prefix'))",
        "layout": "TopToBottom",
        "row-height": "(tryInherit() ?? 27)",
        "row-label-width": "(tryInherit() ?? '40%')",
        "row-value-width": "(tryInherit() ?? '60%')"
    }, {
        "type": "module-checkbox",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "80%",
            "height": "100%",
            "text": "(tryInherit('label') ?? '')",
            "align": "left"
        }, {
            "type": "styled-checkbox",
            "width": "20%",
            "height": "100%",
            "channel": "(inherit('channel-prefix') + '_' + inherit('value-channel'))",
            "checked": "(inherit('value-default'))"
        }]
    }, {
        "type": "module-combobox",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "(inherit('row-label-width'))",
            "height": "100%",
            "text": "(tryInherit('label') ?? '')",
            "align": "left"
        }, {
            "type": "styled-combobox",
            "width": "(inherit('row-value-width'))",
            "height": "100%",
            "items": "(inherit('value-items'))",
            "channel": "(inherit('channel-prefix') + '_' + inherit('value-channel'))"
        }]
    }, {
        "type": "module-group",
        "extends": "styled-group",
        "width": "(constants.module.Width)",
        "height": "100%",
        "border": 0,
        "layout": "TopToBottom"
    }, {
        "type": "module-main-label-and-enabled-checkbox",
        "extends": "styled-group",
        "comment": "Main Label",
        "width": "100%",
        "height": "(1.333 * inherit('row-height'))",
        "border": 0,
        "padding-top": "(inherit('row-height') * 0.167)",
        "padding-left": 8,
        "padding-right": 5,
        "layout": "TopToBottom",
        "background":"(constants.module.color.MainLabel)",
        "children": [{
            "type": "module-checkbox",
            "label": "(inherit('name'))",
            "value-channel": "enabled",
            "value-default": true
        }]
    }, {
        "type": "module-number-box-slider",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "(inherit('row-label-width'))",
            "height": "100%",
            "text": "(tryInherit('label') ?? '')",
            "align": "left"
        }, {
            "type": "styled-number-box-slider",
            "width": "(inherit('row-value-width'))",
            "height": "100%",
            "value-range": "(inherit('value-range'))",
            "value-channel": "(inherit('channel-prefix') + '_' + inherit('value-channel'))"
        }]
    }, {
        "type": "module-row-group",
        "extends": "styled-group",
        "width": "100%",
        "height": "((json['row-count'] + 0.5) * inherit('row-height'))",
        "border": 0,
        "padding-top": "(inherit('row-height') * 0.25)",
        "padding-left": 8,
        "padding-right": 5,
        "layout": "TopToBottom"
    }]
}
