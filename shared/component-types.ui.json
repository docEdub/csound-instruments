{
    "uiTypes": [{
        "type": "component-checkbox",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "padding-left": "(inherit('row-label-padding'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "80%",
            "height": "100%",
            "text": "(inherit('label'))",
            "align": "left"
        }, {
            "type": "styled-checkbox",
            "width": "20%",
            "height": "100%",
            "channel": "(inherit('value-channel'))",
            "checked": "(inherit('value-default'))"
        }]
    }, {
        "type": "component-combobox",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "padding-left": "(inherit('row-label-padding'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "(inherit('row-label-width'))",
            "height": "100%",
            "text": "(inherit('label'))",
            "align": "left"
        }, {
            "type": "styled-combobox",
            "width": "(inherit('row-value-width'))",
            "height": "100%",
            "items": "(inherit('value-items'))",
            "channel": "(inherit('value-channel'))"
        }]
    }, {
        "type": "component-number-box-slider",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "padding-left": "(inherit('row-label-padding'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "(inherit('row-label-width'))",
            "height": "100%",
            "text": "(inherit('label'))",
            "align": "left"
        }, {
            "type": "styled-number-box-slider",
            "width": "(inherit('row-value-width'))",
            "height": "100%",
            "slider-range": "(inherit('value-range'))",
            "slider-channel": "(inherit('value-channel'))"
        }]
    }, {
        "type": "component-full-row-spacer",
        "extends": "styled-spacer",
        "height": "(inherit('row-height'))"
    }, {
        "type": "component-half-row-spacer",
        "extends": "styled-spacer",
        "height": "(inherit('row-height') / 2)"
    }]
}
