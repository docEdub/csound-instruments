{
    "component": {
        "color": {
            "MainLabel": "rgb(128, 0, 0)",

            "BlueGroup": "rgb(32, 32, 96)",
            "GreenGroup": "rgb(0, 48, 0)",
            "GreyGroup": "rgb(56, 56, 56)",
            "PurpleGroup": "rgb(48, 0, 48)",
            "RedGroup": "rgb(48, 0, 0)"
        }
    },
    "uiTypes": [{
        "type": "component-checkbox",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "80%",
            "height": "100%",
            "text": "(inherit('label'))",
            "align": "left"
        }, {
            "type": "styled-checkbox",
            "width": "20%",
            "height": "100%",
            "channel": "(inherit('channel-prefix') + '_' + inherit('value-channel'))",
            "checked": "(inherit('value-default'))"
        }]
    }, {
        "type": "component-combobox",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "(inherit('row-label-width'))",
            "height": "100%",
            "text": "(inherit('label'))",
            "align": "left"
        }, {
            "type": "styled-combobox",
            "width": "(inherit('row-value-width'))",
            "height": "100%",
            "items": "(inherit('value-items'))",
            "channel": "(inherit('channel-prefix') + '_' + inherit('value-channel'))"
        }]
    }, {
        "type": "component-full-row-spacer",
        "extends": "styled-spacer",
        "height": "(inherit('row-height'))"
    }, {
        "type": "component-group",
        "extends": "styled-group",
        "width": "100%",
        "height": "((json['row-count'] + 0.5) * inherit('row-height'))",
        "border": 0,
        "padding-top": "(inherit('row-height') * 0.25)",
        "padding-left": 8,
        "padding-right": 5,
        "layout": "TopToBottom"
    }, {
        "type": "component-half-row-spacer",
        "extends": "styled-spacer",
        "height": "(inherit('row-height') / 2)"
    }, {
        "type": "component-main-label-and-enabled-checkbox",
        "extends": "styled-group",
        "comment": "Main Label",
        "width": "100%",
        "height": "(1.333 * inherit('row-height'))",
        "border": 0,
        "padding-top": "(inherit('row-height') * 0.167)",
        "padding-left": 8,
        "padding-right": 5,
        "layout": "TopToBottom",
        "background":"(constants.component.color.MainLabel)",
        "children": [{
            "type": "component-checkbox",
            "label": "(inherit('name'))",
            "value-channel": "enabled",
            "value-default": true
        }]
    }, {
        "type": "component-number-box-slider",
        "extends": "styled-group",
        "width": "100%",
        "height": "(inherit('row-height'))",
        "layout": "LeftToRight",
        "children": [{
            "type": "styled-label",
            "width": "(inherit('row-label-width'))",
            "height": "100%",
            "text": "(inherit('label'))",
            "align": "left"
        }, {
            "type": "styled-number-box-slider",
            "width": "(inherit('row-value-width'))",
            "height": "100%",
            "value-range": "(inherit('value-range'))",
            "value-channel": "(inherit('channel-prefix') + '_' + inherit('value-channel'))"
        }]
    }]
}
