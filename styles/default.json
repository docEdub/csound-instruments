{
    "color": {
        "Active": "rgb(0, 255, 0)",
        "Background": "rgb(31, 31, 31)",
        "Border": "rgb(0, 63, 127)",
        "Inactive": "rgb(31, 31, 31)",
        "Main": "rgb(0, 63, 127)",
        "TextInput": "black",
        "TextInputBackground": "rgb(121, 150, 179)"
    },
    "uiTypes": [
        {
            "type": "styled-checkbox",
            "extends": "checkbox",
            "border": "eval '(inherit())'",
            "border-color": "eval '(inherit())'",
            "background-color-active": "eval '(constants.color.Active)'",
            "background-color-inactive": "eval '(constants.color.Inactive)'"
        },
        {
            "type": "styled-combobox",
            "extends": "combobox",
            "border": "eval '(inherit())'",
            "border-color": "eval '(inherit())'",
            "background-color-active": "eval '(constants.color.Main)'",
            "background-color-inactive": "eval '(constants.color.Inactive)'"
        },
        {
            "type": "styled-tab-group",
            "extends": "tab-group",
            "border": "eval '(inherit())'",
            "border-color": "eval '(inherit())'",
            "background": "eval '(inherit())'"
        },
        {
            "type": "styled-tab-child",
            "extends": "tab-child",
            "border": "eval '(inherit())'",
            "border-color": "eval '(inherit())'",
            "background": "eval '(inherit())'"
        },
        {
            "type": "styled-tab",
            "extends": "tab",
            "background-disabled": "eval '(inherit('background'))'",
            "background-enabled": "eval '(constants.color.Main)'",
            "font-size": 20,
            "value": "eval '(keyParent.childIndex === 0 ? 1 : 0)'"
        },
        {
            "type": "styled-group",
            "extends": "group",
            "border": "eval '(inherit())'",
            "border-color": "eval '(inherit())'",
            "background": "eval '(inherit())'"
        },
        {
            "type": "styled-console",
            "extends": "console",
            "border": "eval '(inherit())'",
            "border-color": "eval '(inherit())'",
            "background": "eval '(inherit())'",
            "color": "grey"
        },
        {
            "type": "styled-label",
            "extends": "label",
            "background": "eval '(inherit())'",
            "color": "eval '(inherit())'",
            "font-size": 15
        },
        {
            "type": "styled-spacer",
            "extends": "label",
            "background": "eval '(inherit())'",
            "text": "",
            "font-size": 0
        },
        {
            "type": "borderless-number-box-slider",
            "extends": "number-box-slider",
            "border-color": "eval '(inherit('background'))'"
        },
        {
            "type": "styled-number-box-slider",
            "extends": "styled-group",
            "border": "eval '(inherit())'",
            "border-color": "eval '(inherit())'",
            "background": "eval '(inherit())'",
            "children": [{
                "type": "borderless-number-box-slider",
                "width": "100%",
                "height": "100%",
                "background": "eval '(constants.color.TextInputBackground)'",
                "color": "eval '(constants.color.TextInput)'",
                "channel": "eval '(inherit('value-channel'))'",
                "range": "eval '(tryInherit('value-range', { min: 0, max: 1, increment: 0.001 }))'"
            }]
        }
    ]
}
