{
    "colors": {
        "Active": "rgb(255, 0, 0)",
        "Background": "rgb(31, 31, 31)",
        "Border": "rgb(0, 63, 127)",
        "Inactive": "rgb(31, 31, 31)",
        "Main": "rgb(0, 63, 127)",
        "TextInput": "black",
        "TextInputBackground": "rgb(121, 150, 179)"
    },
    "uiTypes": [
        {
            "type": "styled-checkbox",
            "extends": "checkbox",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background-color-active": "(constants.colors.Active)",
            "background-color-inactive": "(constants.colors.Inactive)"
        },
        {
            "type": "styled-combobox",
            "extends": "combobox",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background-color-active": "(constants.colors.Main)",
            "background-color-inactive": "(constants.colors.Inactive)"
        },
        {
            "type": "styled-tab-group",
            "extends": "tab-group",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())"
        },
        {
            "type": "styled-tab-child",
            "extends": "tab-child",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())"
        },
        {
            "type": "styled-tab",
            "extends": "tab",
            "background-disabled": "(inherit('background'))",
            "background-enabled": "(constants.colors.Main)",
            "font-size": 20,
            "value": "(keyParent.childIndex === 0 ? 1 : 0)"
        },
        {
            "type": "styled-group",
            "extends": "group",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())"
        },
        {
            "type": "styled-console",
            "extends": "console",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())",
            "color": "grey"
        },
        {
            "type": "styled-label",
            "extends": "label",
            "background": "(inherit())",
            "color": "(inherit())",
            "font-size": 15
        },
        {
            "type": "styled-spacer",
            "extends": "label",
            "background": "(inherit())",
            "text": "",
            "font-size": 0
        },
        {
            "type": "borderless-number-box-slider",
            "extends": "number-box-slider",
            "border-color": "(inherit('background'))"
        },
        {
            "type": "styled-number-box-slider",
            "extends": "styled-group",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())",
            "children": [{
                "type": "borderless-number-box-slider",
                "width": "100%",
                "height": "100%",
                "background": "(constants.colors.TextInputBackground)",
                "color": "(constants.colors.TextInput)",
                "channel": "(inherit('value-channel'))",
                "range": "(tryInherit('value-range', { min: 0, max: 1, increment: 0.001 }))"
            }]
        }
    ]
}
