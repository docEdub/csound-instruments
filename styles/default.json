{
    "colors": {
        "Active": "rgb(255, 0, 0)",
        "Background": "rgb(31, 31, 31)",
        "Border": "rgb(0, 63, 127)",
        "Inactive": "rgb(31, 31, 31)",
        "Main": "rgb(0, 63, 127)"
    },
    "uiTypes": [
        {
            "type": "styled-checkbox",
            "extends": "checkbox",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background-color-active": "(global.json.colors.Active)",
            "background-color-inactive": "(global.json.colors.Inactive)"
        },
        {
            "type": "styled-combobox",
            "extends": "combobox",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background-color-active": "(global.json.colors.Main)",
            "background-color-inactive": "(global.json.colors.Inactive)"
        },
        {
            "type": "styled-tab-group",
            "extends": "tab-group",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())"
        },
        {
            "type": "styled-tab-child",
            "extends": "tab-child",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())"
        },
        {
            "type": "styled-tab",
            "extends": "tab",
            "background-disabled": "(inherit('background'))",
            "background-enabled": "(global.json.colors.Main)",
            "font-size": 20,
            "value": "(keyParent.childIndex === 0 ? 1 : 0)"
        },
        {
            "type": "styled-group",
            "extends": "group",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())"
        },
        {
            "type": "styled-console",
            "extends": "console",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())",
            "color": "grey"
        },
        {
            "type": "styled-label",
            "extends": "label",
            "background": "(inherit())",
            "color": "(inherit())",
            "font-size": 15
        },
        {
            "type": "styled-spacer",
            "extends": "label",
            "background": "(inherit())",
            "text": "",
            "font-size": 0
        },
        {
            "type": "borderless-number-box-slider",
            "extends": "number-box-slider",
            "border-color": "(inherit('background'))"
        },
        {
            "type": "styled-number-box-slider",
            "extends": "styled-group",
            "border": "(inherit())",
            "border-color": "(inherit())",
            "background": "(inherit())",
            "children": [{
                "type": "borderless-number-box-slider",
                "width": "100%",
                "height": "100%",
                "color": "(inherit())",
                "channel": "(inherit('slider-channel'))",
                "range": {
                    "min": 0,
                    "max": 1,
                    "increment": 0.001
                }
            }]
        }
    ]
}