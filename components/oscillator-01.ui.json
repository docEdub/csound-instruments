{
    "oscillator": {
        "wave": {
            "Sawtooth": 0,
            "Square": 1
        }
    },
    "uiTypes": [{
        "type": "oscillator-component",
        "comment": "Oscillator component",
        "extends": "styled-group",
        "width": 200,
        "height": "100%",
        "channel-prefix": "(tryInherit('channel-prefix') ?? 'oscillator')",
        "layout": "TopToBottom",
        "children": [{
            "comment": "Wave 1",
            "type": "styled-label",
            "width": "100%",
            "height": 30,
            "text": "Oscillator 1"
        }, {
            "type": "styled-group",
            "width": "100%",
            "height": 30,
            "padding-left": 10,
            "layout": "LeftToRight",
            "children": [{
                "type": "styled-label",
                "width": "40%",
                "height": "100%",
                "text": "Wave",
                "align": "left"
            }, {
                "type": "styled-combobox",
                "width": "60%",
                "height": "100%",
                "items": "(Object.keys(global.json.oscillator.wave))",
                "channel": "(inherit('channel-prefix') + '_wave_1')"
            }]
        }]
    }]
}
