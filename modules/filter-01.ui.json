{
    "Filter_A": {
        "Channel": {
            "Enabled": 1,
            "FilterType": 2,
            "Cutoff": 3,
            "Q": 4,
            "ProcessingType": 5,
            "Saturation": 6,
            "EnvelopeAmount": 7,
            "EnvelopeAttack": 8,
            "EnvelopeDecay": 9,
            "EnvelopeSustain": 10,
            "EnvelopeRelease": 11
        },
        "FilterType": {
            "LowPass": 1,
            "HighPass": 2
        },
        "ProcessingType": {
            "None": 1,
            "Nonlinear": 2
        }
    },
    "uiTypes": [
        {
            "type": "filter-a-module",
            "extends": "module",
            "height": 403,
            "children": [
                {
                    "type": "module-main-label-and-enabled-checkbox",
                    "value-channel": "Enabled",
                    "value-default": true
                },
                {
                    "type": "module-row-group",
                    "background": "eval '(Constants.Module.Color.PurpleGroup)'",
                    "row-count": 13,
                    "children": [
                        {
                            "comment": "Type",
                            "type": "module-combobox",
                            "label": "Type",
                            "value-channel": "FilterType",
                            "value-items": "eval '(Object.keys(Constants.Filter_A.FilterType))'"
                        },
                        {
                            "comment": "Cutoff",
                            "type": "module-number-box-slider",
                            "label": "Cutoff",
                            "value-channel": "Cutoff",
                            "value-range": { "min": 0, "max": 1, "value": 1, "step": 0.001 }
                        },
                        {
                            "comment": "Q",
                            "type": "module-number-box-slider",
                            "label": "Q",
                            "value-channel": "Q",
                            "value-range": { "min": 1, "max": 10, "value": 1, "step": 0.01 }
                        },
                        {
                            "type": "module-row-spacer"
                        },
                        {
                            "comment": "Processing",
                            "type": "module-label",
                            "label": "Processing"
                        }, {
                            "type": "module-combobox",
                            "value-channel": "ProcessingType",
                            "value-items": "eval '(Object.keys(Constants.Filter_A.ProcessingType))'"
                        },
                        {
                            "comment": "Saturation",
                            "type": "module-label",
                            "label": "Saturation"
                        },
                        {
                            "type": "module-number-box-slider",
                            "value-channel": "Saturation",
                            "value-range": { "min": 1, "max": 10, "value": 1, "step": 0.01 }
                        },
                        {
                            "type": "module-row-spacer"
                        },
                        {
                            "comment": "Envelope Amount",
                            "type": "module-number-box-slider",
                            "label": "Env",
                            "value-channel": "EnvelopeAmount",
                            "value-range": { "min": 0, "max": 1, "value": 0, "step": 0.01 }
                        },
                        {
                            "comment": "Attack",
                            "type": "module-number-box-slider",
                            "label": "A",
                            "value-channel": "EnvelopeAttack",
                            "value-range": { "min": 0.001, "max": 10, "value": 0.01, "step": 0.01 }
                        },
                        {
                            "comment": "Decay",
                            "type": "module-number-box-slider",
                            "label": "D",
                            "value-channel": "EnvelopeDecay",
                            "value-range": { "min": 0, "max": 10, "value": 0.01, "step": 0.01 }
                        },
                        {
                            "comment": "Sustain",
                            "type": "module-number-box-slider",
                            "label": "S",
                            "value-channel": "EnvelopeSustain",
                            "value-range": { "min": 0, "max": 1, "value": 1, "step": 1 }
                        },
                        {
                            "comment": "Release",
                            "type": "module-number-box-slider",
                            "label": "R",
                            "value-channel": "EnvelopeRelease",
                            "value-range": { "min": 0, "max": 10, "value": 0.01, "step": 0.01 }
                        }
                    ]
                }
            ]
        }
    ]
}
