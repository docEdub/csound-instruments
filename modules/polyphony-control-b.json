{
    "PolyphonyControl_B": {
        "MaxNoteCount": 4096,
        "Channel": {
            "Enabled": 1,
            "SoftMax": 2,
            "HardMax": 3,
            "SoftOffFadeTime": 4,
            "SoftOnFadeTime": 5,
            "KeepHighNote": 6,
            "KeepLowNote": 7,
            "SoftCount": 8,
            "HardCount": 9
        },
        "ChannelDefault": {
            "SoftMax": 90,
            "HardMax": 100,
            "SoftOffFadeTime": 1,
            "KeepHighNote": 0,
            "KeepLowNote": 0
        },
        "State": {
            "Initialized": 0,
            "On": 1,
            "SoftOn": 2,
            "SoftOff": 3,
            "HardOff": 4,
            "Muted": 5,
            "Off": 6
        },
        "Instance": {
            "Enabled": 0,
            "SoftMax": 1,
            "HardMax": 2,
            "SoftOffFadeTime": 3,
            "SoftOnFadeTime": 4,
            "KeepHighNote": 5,
            "KeepLowNote": 6,
            "NextNoteId": 7,
            "NoteCount": 8,
            "UpdateSoftNotes": 9,
            "UpdateHardNotes": 10,
            "RemoveFinishedNotes": 11,
            "HighNoteNumber": 12,
            "LowNoteNumber": 13,
            "HardOffActiveNoteCount": 14,
            "SoftOffActiveNoteCount": 15
        },
        "InstanceMemberCount": 16,
        "Note": {
            "Id": 0,
            "Duration": 1,
            "Number": 2,
            "Velocity": 3,
            "State": 6,
            "Amp": 7,
            "SoftOffActivated": 8,
            "HardOffActivated": 9
        },
        "NoteMemberCount": 10
    },
    "uiTypes": [
        {
            "type": "polyphony-control-b-module",
            "extends": "module",
            "height": 250,
            "row-label-width": "60%",
            "row-value-width": "40%",
            "children": [
                {
                    "type": "module-main-label-and-enabled-checkbox",
                    "value-default": "(tryInherit('value-default') ?? false)"
                },
                {
                    "type": "module-row-group",
                    "background": "(Constants.Module.Color.BlueGroup)",
                    "row-count": 5,
                    "children": [
                        {
                            "label": "Soft max",
                            "type": "module-number-box-slider",
                            "value-channel": "SoftMax",
                            "value-range": {
                                "min": 1,
                                "max": 100,
                                "value": "(Constants.PolyphonyControl_B.ChannelDefault.SoftMax)",
                                "step": 1
                            }
                        },
                        {
                            "label": "Hard max",
                            "type": "module-number-box-slider",
                            "value-channel": "HardMax",
                            "value-range": {
                                "min": 1,
                                "max": 100,
                                "value": "(Constants.PolyphonyControl_B.ChannelDefault.HardMax)",
                                "step": 1
                            }
                        },
                        {
                            "label": "Fade time",
                            "type": "module-number-box-slider",
                            "value-channel": "SoftOffFadeTime",
                            "value-range": {
                                "min": 0,
                                "max": 10,
                                "value": "(Constants.PolyphonyControl_B.ChannelDefault.SoftOffFadeTime)",
                                "step": 0.1
                            }
                        },
                        {
                            "label": "Keep high",
                            "type": "module-checkbox",
                            "value-channel": "KeepHighNote",
                            "value-default": "(Constants.PolyphonyControl_B.ChannelDefault[json['value-channel']])"
                        },
                        {
                            "label": "Keep low",
                            "type": "module-checkbox",
                            "value-channel": "KeepLowNote",
                            "value-default": "(Constants.PolyphonyControl_B.ChannelDefault[json['value-channel']])"
                        }
                    ]
                }
            ]
        }
    ]
}
