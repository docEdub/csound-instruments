{
    "Source_A": {
        "Channel": {
            "Enabled": 1,
            "MainAmp": 2,
            "Osc1Enabled": 3,
            "Osc1Wave": 4,
            "Osc1PulseWidth": 5,
            "Osc1Fine": 6,
            "OscMix": 7,
            "Osc2Enabled": 8,
            "Osc2Link": 9,
            "Osc2Wave": 10,
            "Osc2PulseWidth": 11,
            "Osc2Semi": 12,
            "Osc2Fine": 13,
            "SubEnabled": 14,
            "SubWave": 15,
            "SubAmp": 16
        },
        "Osc": {
            "Wave": {
                "Saw": 1,
                "Pulse": 2,
                "Triangle": 3,
                "Sine": 4
            }
        },
        "Sub": {
            "Wave": {
                "Pulse": 1,
                "Triangle": 2
            }
        }
    },
    "uiTypes": [{
        "type": "source-a-module",
        "extends": "module",
        "height": 513,
        "children": [{
            "type": "module-main-label-and-enabled-checkbox",
            "value-channel": "Enabled",
            "value-default": true
        }, {
            "comment": "Main Amp",
            "type": "module-row-group",
            "background": "eval '(Constants.Module.Color.RedGroup)'",
            "row-count": 1,
            "children": [{
                "type": "module-number-box-slider",
                "label": "Amp",
                "value-channel": "MainAmp",
                "value-range": { "min": 0, "max": 1, "value": 0.25, "step": 0.01 }
            }]
        }, {
            "comment": "Oscillator 1",
            "type": "module-row-group",
            "background": "eval '(Constants.Module.Color.BlueGroup)'",
            "row-count": 4,
            "children": [{
                "type": "module-checkbox",
                "label": "Oscillator 1",
                "value-default": true,
                "value-channel": "Osc1Enabled"
            }, {
                "comment": "Wave 1",
                "type": "module-combobox",
                "label": "Wave",
                "value-channel": "Osc1Wave",
                "value-items": "eval '(Object.keys(Constants.Source_A.Osc.Wave))'"
            }, {
                "comment": "Pulse Width 1",
                "type": "module-number-box-slider",
                "label": "PW",
                "value-channel": "Osc1PulseWidth",
                "value-range": { "min": 0.01, "max": 0.5, "value": 0.5, "step": 0.01 }
            }, {
                "comment": "Fine 1",
                "type": "module-number-box-slider",
                "label": "Fine",
                "value-channel": "Osc1Fine",
                "value-range": { "min": 0, "max": 30, "value": 0, "step": 1 }
            }]
        }, {
            "comment": "Mix",
            "type": "module-row-group",
            "background": "eval '(Constants.Module.Color.GreenGroup)'",
            "row-count": 1,
            "children": [{
                "type": "module-number-box-slider",
                "label": "Mix",
                "value-channel": "OscMix",
                "value-range": { "min": 0, "max": 1, "value": 0.5, "step": 0.01 }
            }]
        }, {
            "comment": "Oscillator 2",
            "type": "module-row-group",
            "background": "eval '(Constants.Module.Color.PurpleGroup)'",
            "row-count": 6,
            "children": [{
                "type": "module-checkbox",
                "label": "Oscillator 2",
                "value-channel": "Osc2Enabled",
                "value-default": true
            }, {
                "comment": "Link",
                "type": "module-checkbox",
                "label": "Link",
                "value-channel": "Osc2Link",
                "value-default": true
            }, {
                "comment": "Wave 2",
                "type": "module-combobox",
                "label": "Wave",
                "value-channel": "Osc2Wave",
                "value-items": "eval '(Object.keys(Constants.Source_A.Osc.Wave))'"
            }, {
                "comment": "Pulse Width 2",
                "type": "module-number-box-slider",
                "label": "PW",
                "value-channel": "Osc2PulseWidth",
                "value-range": { "min": 0.01, "max": 0.5, "value": 0.5, "step": 0.01 }
            }, {
                "comment": "Semitone 2",
                "type": "module-number-box-slider",
                "label": "Semi",
                "value-channel": "Osc2Semi",
                "value-range": { "min": 0, "max": 36, "value": 0, "step": 1 }
            }, {
                "comment": "Fine 2",
                "type": "module-number-box-slider",
                "label": "Fine",
                "value-channel": "Osc2Fine",
                "value-range": { "min": -30, "max": 0, "value": 0, "step": 1 }
            }]
        }, {
            "comment": "Sub",
            "type": "module-row-group",
            "background": "eval '(Constants.Module.Color.GreyGroup)'",
            "row-count": 3,
            "children": [{
                "type": "module-checkbox",
                "label": "Sub",
                "value-channel": "SubEnabled",
                "value-default": false
            }, {
                "comment": "Sub Wave",
                "type": "module-combobox",
                "label": "Wave",
                "value-channel": "SubWave",
                "value-items": "eval '(Object.keys(Constants.Source_A.Sub.Wave))'"
            }, {
                "comment": "Sub Amp",
                "type": "module-number-box-slider",
                "label": "Amp",
                "value-channel": "SubAmp",
                "value-range": { "min": 0, "max": 2, "value": 0.5, "step": 0.01 }
            }]
        }]
    }]
}
