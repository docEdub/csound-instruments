{
    "Clamp_A": {
        "Channel": {
            "Enabled": 1,
            "Min": 2,
            "Max": 3
        }
    },
    "uiTypes": [
        {
            "type": "clamp-a-module",
            "extends": "module",
            "height": 106,
            "children": [
                {
                    "type": "module-main-label-and-enabled-checkbox",
                    "value-default": false
                },
                {
                    "type": "module-row-group",
                    "background": "(Constants.Module.Color.GreenGroup)",
                    "row-count": 2,
                    "children": [
                        {
                            "label": "Min",
                            "type": "module-number-box-slider",
                            "value-range": { "min": -1, "max": 1, "value": -1, "step": 0.01 }
                        },
                        {
                            "label": "Max",
                            "type": "module-number-box-slider",
                            "value-range": { "min": -1, "max": 1, "value": 1, "step": 0.01 }
                        }
                    ]
                }
            ]
        }
    ]
}
