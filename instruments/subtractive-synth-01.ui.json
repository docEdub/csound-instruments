{
    "cabbage-output-config": {
        "debug": false
    },
    "ui": {
        "type": "form",
        "name": "Subtractive Synth 01",
        "width": 800,
        "height": 600,
        "border": 1,
        "border-color": "(global.json.colors.Border)",
        "background": "black",
        "color": "white",
        "id": "ss01",
        "children": [{
            "type": "styled-tab-group",
            "id": "main-tabs",
            "width": "100%",
            "height": "100%",
            "tab-height": 25,
            "children": [{
                "comment": "UI tab",
                "type": "styled-tab-child",
                "height": "100%",
                "width": "100%",
                "layout": "TopToBottom",
                "tab": {
                    "type": "styled-tab",
                    "text": "UI",
                    "height": "(inherit('tab-height'))",
                    "width": 100
                },
                "children": [{
                    "comment": "Components",
                    "type": "styled-group",
                    "width": "100%",
                    "height": "100%",
                    "layout": "LeftToRight",
                    "children": [{
                        "type": "oscillator-component"
                    }]
                }]
            }, {
                "comment": "Console tab",
                "type": "styled-tab-child",
                "height": "100%",
                "width": "100%",
                "tab": {
                    "type": "styled-tab",
                    "text": "Console",
                    "height": "(inherit('tab-height'))",
                    "width": 80
                },
                "children": [{
                    "type": "styled-console",
                    "width": "100%",
                    "height": "100%"
                }]
            }]
        }]
    }
}
